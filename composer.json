{
    "name": "nepada/email-address",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Email address value object.",
    "keywords": ["email address", "value object", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/email-address/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "nette/utils": "^3.2@dev || ^4.0@dev"
    },
    "require-dev": {
        "nette/tester": "v2.5.4",
        "phpstan/phpstan": "2.1.11",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "phpstan/phpstan-nette": "2.0.3",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.4.1",
        "shipmonk/phpstan-rules": "4.1.1",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.14.1"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\EmailAddress\\": "src/EmailAddress/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev"
        }
    }
}
